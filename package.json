{
  "name": "expo-template-kickstart",
  "version": "1.0.1",
  "main": "src/main.ts",
  "scripts": {
    "prepare": "husky install",
    "start": "expo start",
    "lint": "yarn lint:es && yarn lint:tsc",
    "lint:es": "eslint ./src",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "format": "eslint ./src --fix",
    "plop": "plop",
    "icons:update": "node ./dev/extractIconName.js"
  },
  "eslintConfig": {
    "extends": [
      "shiny-code/react-native"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
  "dependencies": {
    "expo": "^48.0.0",
    "expo-linking": "~4.0.1",
    "expo-router": "^1.0.0",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.70.6",
    "eslint": "^8.0.0",
    "eslint-config-shiny-code": "^1.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.0",
    "lint-staged": "^13.1.0",
    "plop": "^2.7.4",
    "prettier": "^2.7.1",
    "ts-jest": "^26.0.2",
    "typescript": "^4.9.4"
  }
}
