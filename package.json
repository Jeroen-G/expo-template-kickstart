{
  "name": "expo-template-kickstart",
  "version": "1.1.0",
  "main": "expo-router/entry",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "start": "expo start",
    "lint": "yarn lint:es && yarn lint:tsc",
    "lint:es": "eslint ./src",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "format": "eslint ./src --fix",
    "plop": "plop",
    "icons:update": "node ./dev/extractIconName.js"
  },
  "eslintConfig": {
    "extends": [
      "shiny-code/react-native"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.2",
    "expo": "^49.0.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.2.14",
    "eslint": "^8.0.0",
    "eslint-config-shiny-code": "^1.2",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "lint-staged": "^13.1.0",
    "plop": "^2.7.4",
    "prettier": "^2.7.1",
    "typescript": "^5.1.3"
  }
}
