{
  "name": "expo-template-kickstart",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "prepare": "husky install",
    "start": "expo start",
    "lint": "yarn lint:es && yarn lint:tsc",
    "lint:es": "eslint ./src",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "format": "eslint ./src --fix",
    "plop": "plop",
    "icons:update": "node ./dev/extractIconName.js"
  },
  "eslintConfig": {
    "extends": [
      "shiny-code/react-native"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
  "dependencies": {
    "expo": "~47.0.12",
    "expo-linking": "~3.3.0",
    "expo-router": "^1.0.0",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "eslint": "^8.0.0",
    "eslint-config-shiny-code": "^1.1",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "jest-expo": "^47.0.1",
    "lint-staged": "^13.1.0",
    "plop": "^2.7.4",
    "prettier": "^2.7.1",
    "ts-jest": "^26.0.2",
    "typescript": "^4.6.3"
  }
}
